{
  "name": "BURelyFoundation",
  "version": "0.0.1.20",
  "license": "MIT",
  "summary": "BURelyFoundation is the basic library for byte ads",
  "description": "BURelyFoundation is the basic library for byte ads",
  "homepage": "https://github.com/bytedance/Bytedance-UnionAD",
  "authors": {
    "zhangtianhao": "zhangtianhao.1230@bytedance.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "http": "https://sf3-fe-tos.pglstatp-toutiao.com/obj/bu-rely-framework/0.0.1.20/Rely.zip"
  },
  "frameworks": [
    "UIKit",
    "CoreFoundation",
    "WebKit",
    "StoreKit",
    "CoreServices",
    "MediaPlayer",
    "CoreMedia",
    "CoreLocation",
    "AVFoundation",
    "CoreTelephony",
    "SystemConfiguration",
    "AdSupport",
    "CoreMotion",
    "Accelerate",
    "Security"
  ],
  "weak_frameworks": "AppTrackingTransparency",
  "libraries": [
    "z",
    "bz2",
    "resolv.9",
    "c++",
    "sqlite3",
    "xml2",
    "c++abi"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "OTHER_LDFLAGS": "-ObjC",
    "COMPILER_INDEX_STORE_ENABLE": "NO"
  },
  "default_subspecs": [
    "HM",
    "BUNETWork"
  ],
  "subspecs": [
    {
      "name": "HM",
      "vendored_frameworks": [
        "Rely/HM.framework"
      ],
      "preserve_paths": "Rely/HM.framework"
    },
    {
      "name": "AFN",
      "public_header_files": "Rely/AFNetworking/*.h",
      "source_files": "Rely/AFNetworking/*.{h,m}"
    },
    {
      "name": "BUNETWork",
      "public_header_files": "Rely/BUNETWork/Headers/*.h",
      "source_files": "Rely/BUNETWork/Headers/*.{h,m}",
      "vendored_libraries": "Rely/BUNETWork/libBUNETWork.a"
    }
  ]
}
