{
  "name": "BoomCore",
  "version": "1.0.1",
  "summary": "BoomCore is Boom core SDK",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "805413147@qq.com": "mybadge@163.com"
  },
  "homepage": "https://github.com/mybadge/BoomCore",
  "source": {
    "git": "https://github.com/mybadge/BoomCore.git",
    "tag": "1.0.1"
  },
  "description": "this is Boom Core api, includes RLoud, BRTC, 是一个重要的开发工具包",
  "platforms": {
    "ios": "10.0"
  },
  "requires_arc": true,
  "frameworks": [
    "UIKit",
    "AVFoundation",
    "Foundation",
    "AudioToolbox",
    "CoreAudio",
    "CoreGraphics",
    "CoreVideo",
    "GLKit",
    "VideoToolbox"
  ],
  "libraries": [
    "c",
    "sqlite3",
    "stdc++"
  ],
  "xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES",
    "ENABLE_BITCODE": "YES",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) PODSPEC_NAME=#{s.name} PODSPEC_VERSION=#{s.version}"
  },
  "ios": {
    "vendored_frameworks": [
      "ios/BoomCore.framework",
      "ios/Vloud.framework"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  }
}
