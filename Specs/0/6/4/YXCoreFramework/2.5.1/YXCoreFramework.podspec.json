{
  "name": "YXCoreFramework",
  "platforms": {
    "ios": "7.0"
  },
  "version": "2.5.1",
  "license": "MIT",
  "summary": "A base framework of creditease",
  "homepage": "http://gitlab.creditease.corp/ios_block/yxcoreframework",
  "authors": {
    "宋玉涛": "yutaosong@creditease.cn"
  },
  "source": {
    "git": "http://gitlab.creditease.corp/ios_block/yxcoreframework",
    "tag": "2.5.1"
  },
  "requires_arc": true,
  "xcconfig": {
    "OTHER_LDFLAGS": "-lObjC"
  },
  "subspecs": [
    {
      "name": "YXCoreFramework",
      "source_files": [
        "YXCoreFramework/YXCoreFramework/Classes/UI/View/*.{h,m}",
        "YXCoreFramework/YXCoreFramework/Classes/UI/Controller/*.{h,m}",
        "YXCoreFramework/YXCoreFramework/Classes/YXCore/*.{h}",
        "YXCoreFramework/YXCoreFramework/Classes/UI/View/YTGuidView/*.{h,m}"
      ],
      "frameworks": [
        "UIKit"
      ],
      "dependencies": {
      },
      "subspecs": [
        {
          "name": "YTImageBrowser",
          "source_files": [
            "YXCoreFramework/YXCoreFramework/Classes/UI/View/YTImageBrowser/*.{h,m}"
          ]
        },
        {
          "name": "YXUtil",
          "requires_arc": [
            "YXCoreFramework/YXCoreFramework/Classes/YXUtil/Category/*.{h,m}",
            "YXCoreFramework/YXCoreFramework/Classes/YXUtil/Util/*.{h,m}"
          ],
          "source_files": [
            "YXCoreFramework/YXCoreFramework/Classes/YXUtil/Category/*.{h,m}",
            "YXCoreFramework/YXCoreFramework/Classes/YXUtil/Util/*.{h,m}",
            "YXCoreFramework/YXCoreFramework/Classes/YXUtil/Util/KeychainItemWrapper/*.{h,m}"
          ],
          "subspecs": [
            {
              "name": "libqrencode",
              "source_files": [
                "YXCoreFramework/YXCoreFramework/Classes/YXUtil/Util/libqrencode/*.{h,c}"
              ]
            }
          ]
        }
      ]
    }
  ]
