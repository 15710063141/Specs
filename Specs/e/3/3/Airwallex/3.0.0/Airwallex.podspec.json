{
  "name": "Airwallex",
  "version": "3.0.0",
  "summary": "Integrate Airwallex into your iOS app",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "homepage": "https://www.airwallex.com/docs/#overview",
  "authors": {
    "Airwallex": "pa_mobile_sdk@airwallex.com"
  },
  "source": {
    "git": "https://github.com/airwallex/airwallex-payment-ios.git",
    "tag": "3.0.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "static_framework": true,
  "default_subspecs": [
    "Core",
    "WeChatPay",
    "Card",
    "Redirect"
  ],
  "resources": [
    "Airwallex/Airwallex/Resources/**/*"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Airwallex/Airwallex/Sources/Core/*.{h,m}",
        "Airwallex/Airwallex/Sources/Core/*.{h,m}",
        "Airwallex/Airwallex/Sources/UI/*.{h,m}",
        "Airwallex/Airwallex/Sources/UI/*.{h,m}"
      ],
      "dependencies": {
        "AirwallexBehavioralBiometrics": [

        ],
        "AirwallexProfiling": [

        ],
        "AirwallexProfilingConnections": [

        ]
      }
    },
    {
      "name": "WeChatPay",
      "dependencies": {
        "Airwallex/Core": [

        ],
        "WechatOpenSDK": [

        ]
      },
      "source_files": "Airwallex/Airwallex/Sources/WeChatPay/*.{h,m}"
    },
    {
      "name": "Card",
      "dependencies": {
        "Airwallex/Core": [

        ],
        "AirwallexCardinalMobile": [

        ]
      },
      "source_files": "Airwallex/Airwallex/Sources/Card/*.{h,m}"
    },
    {
      "name": "Redirect",
      "dependencies": {
        "Airwallex/Core": [

        ]
      },
      "source_files": "Airwallex/Airwallex/Sources/Redirect/*.{h,m}"
    }
  ]
}
