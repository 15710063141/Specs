{
  "name": "BUHMFoundation",
  "version": "0.0.0.60",
  "license": "MIT",
  "summary": "BURelyFoundation is the basic library for byte ads",
  "description": "BURelyFoundation is the basic library for byte ads",
  "homepage": "https://github.com/bytedance/Bytedance-UnionAD",
  "authors": {
    "zhangtianhao": "zhangtianhao.1230@bytedance.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "git@code.byted.org:TTIOS/tt_sdk_rely_demo.git",
    "branch": "dev_2000"
  },
  "frameworks": [
    "UIKit",
    "CoreFoundation",
    "WebKit",
    "StoreKit",
    "CoreServices",
    "MediaPlayer",
    "CoreMedia",
    "CoreLocation",
    "AVFoundation",
    "CoreTelephony",
    "SystemConfiguration",
    "AdSupport",
    "CoreMotion",
    "Accelerate",
    "Security"
  ],
  "weak_frameworks": "AppTrackingTransparency",
  "libraries": [
    "z",
    "bz2",
    "resolv.9",
    "c++",
    "sqlite3",
    "xml2",
    "c++abi"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "OTHER_LDFLAGS": "-ObjC",
    "COMPILER_INDEX_STORE_ENABLE": "NO"
  },
  "default_subspecs": [
    "HM",
    "AFN"
  ],
  "subspecs": [
    {
      "name": "HM",
      "vendored_frameworks": [
        "Rely/HM.framework"
      ],
      "preserve_paths": "Rely/HM.framework"
    },
    {
      "name": "AFN",
      "public_header_files": "BUHMFoundation/AFNetworking/*.h",
      "source_files": "BUHMFoundation/AFNetworking/*.{h,m}"
    }
  ]
}
