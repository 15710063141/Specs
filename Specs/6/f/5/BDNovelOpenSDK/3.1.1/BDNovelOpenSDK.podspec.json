{
  "name": "BDNovelOpenSDK",
  "version": "3.1.1",
  "platforms": {
    "ios": "9.0"
  },
  "license": "MIT",
  "summary": "BDNovelOpenSDK",
  "homepage": "https://bytedance.com/",
  "authors": {
    "liuyunxuan": "liuyunxuan@bytedance.com"
  },
  "source": {
    "http": "http://sf3-fe-tos.pglstatp-toutiao.com/obj/pangle-empower/pangrowh-novel/3.1.1/BDNovelOpenSDK-3.1.1.zip"
  },
  "description": "This library provides ability for App enable Novel Business",
  "frameworks": [
    "UIKit",
    "WebKit",
    "SystemConfiguration"
  ],
  "preserve_paths": [
    "BDNovelOpenSDK/include/*",
    "BDNovelOpenSDK/include/**/*",
    "BDNovelOpenSDK/other/*",
    "BDNovelOpenSDK/other/**/*",
    "BDNovelOpenSDK/novel/*"
  ],
  "ios": {
    "vendored_libraries": [
      "BDNovelOpenSDK/other/*.a",
      "BDNovelOpenSDK/other/**/*.a",
      "BDNovelOpenSDK/novel/*.a",
      "BDNovelOpenSDK/novel/sdk/*.a"
    ]
  },
  "source_files": "BDNovelOpenSDK/include/*.h",
  "public_header_files": "BDNovelOpenSDK/include/*.h",
  "libraries": [
    "c++",
    "resolv",
    "z",
    "sqlite3",
    "bz2",
    "xml2",
    "c++abi"
  ],
  "resources": "BDNovelOpenSDK/novel/bundle/BDNovelResource.bundle",
  "dependencies": {
    "Ads-CN/BUAdSDK": [
      ">=3.6.1.5"
    ],
    "Ads-CN/Domestic": [
      ">=3.6.1.5"
    ],
    "Ads-CN/BUFoundation": [
      ">=3.6.1.5"
    ],
    "FMDB/standard": [

    ],
    "YYCache": [

    ],
    "YYText": [

    ],
    "MMKV": [

    ]
  },
  "xcconfig": {
    "USE_HEADERMAP": "NO",
    "HEADER_SEARCH_PATHS": "$(inherited) \"${PODS_ROOT}/BDNovelOpenSDK/include\"",
    "ALWAYS_SEARCH_USER_PATHS": "NO"
  },
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  }
}
