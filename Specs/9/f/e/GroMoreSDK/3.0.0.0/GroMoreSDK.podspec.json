  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "default_subspecs": "ABUAdSDK",
  "subspecs": [
    {
      "name": "ABUAdSDK",
      "vendored_frameworks": [
        "GroMoreSDK/ABUAdSDK.framework"
      ],
      "preserve_paths": "GroMoreSDK/ABUAdSDK.framework",
      "dependencies": {
        "Ads-CN": [
          "3.9.0.4"
        ]
      }
    }
  ]
}
