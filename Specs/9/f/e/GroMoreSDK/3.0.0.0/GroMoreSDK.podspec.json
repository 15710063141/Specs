{
  "name": "GroMoreSDK",
  "version": "3.0.0.0",
  "summary": "GroMoreSDK is a Ad SDK from Bytedance providing content service.",
  "description": "Bytedance provides content which include rewardVideo、fullscreenVideo、banner etc.\nAnd provide aggregation services of various mainstream advertising SDKs",
  "homepage": "https://www.pangleglobal.com/zh",
  "source": {
    "http": "http://sf3-fe-tos.pglstatp-toutiao.com/obj/pangle-gromoresdk/GroMoreSDK/3.0.0.0/GroMoreSDK.zip"
  },
  "license": {
    "type": "MIT",
    "file": "GroMoreSDK/LICENSE"
  },
  "authors": {
    "wangchao": "wangchao@bytedance.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "frameworks": [
    "UIKit",
    "CoreMotion",
    "CoreLocation",
    "SystemConfiguration",
    "CoreGraphics",
    "CoreMotion",
    "CoreTelephony",
    "AdSupport",
    "SystemConfiguration",
    "QuartzCore",
    "WebKit",
    "MessageUI",
    "SafariServices",
    "AVFoundation",
    "EventKit",
    "QuartzCore",
    "CoreMedia",
