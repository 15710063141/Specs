{
  "name": "XDC",
  "version": "0.1.0",
  "summary": "XDC cocoapod library",
  "description": "Xinfin sdk to connect with xinfin network",
  "homepage": "https://github.com/angshuman2611/XCD.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "angshuman2611": "angshuman@leewayhertz.com"
  },
  "source": {
    "git": "https://github.com/angshuman2611/XCD.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5.0",
  "source_files": [
    "Classes/**/*.swift",
    "Classes/lib/**/*.{c,h}"
  ],
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS[sdk=iphonesimulator*]": "$(PODS_TARGET_SRCROOT)/Classes/lib/**",
    "SWIFT_INCLUDE_PATHS[sdk=iphoneos*]": "$(PODS_TARGET_SRCROOT)/Classes/lib/**"
  },
  "preserve_paths": "Classes/lib/**/module.map",
  "dependencies": {
    "GenericJSON": [
      "~> 2.0.0"
    ],
    "BigInt": [
      "~> 5.2.0"
    ],
    "secp256k1.swift": [
      "~> 0.1.0"
    ]
  },
  "swift_version": "5.0"
}
