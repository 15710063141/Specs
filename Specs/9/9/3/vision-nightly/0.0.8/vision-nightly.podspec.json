{
  "name": "vision-nightly",
  "version": "0.0.8",
  "authors": "PyTorch Team",
  "license": {
    "type": "BSD"
  },
  "homepage": "https://github.com/pytorch/vision",
  "source": {
    "http": "https://ossci-ios-build.s3.amazonaws.com/libtorchvision_ops_ios_nightly_build_with_torch.zip"
  },
  "summary": "The C++ library of TorchVision ops for iOS",
  "description": "The C++ library of TorchVision ops for iOS.",
  "static_framework": true,
  "platforms": {
    "ios": "12.0"
  },
  "header_mappings_dir": "install/include/",
  "preserve_paths": "install/include/**/*.{h,cpp,cc,c}",
  "vendored_libraries": "install/lib/*.a",
  "libraries": [
    "c++",
    "stdc++"
  ],
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(inherited) \"$(PODS_ROOT)/vision-nightly/install/include/\"",
    "OTHER_LDFLAGS": "-force_load \"$(PODS_ROOT)/vision-nightly/install/lib/libtorch.a\" -force_load \"$(PODS_ROOT)/vision-nightly/install/lib/libtorch_cpu.a\" -force_load \"$(PODS_ROOT)/vision-nightly/install/lib/libtorchvision_ops.a\"",
    "VALID_ARCHS": "x86_64 arm64",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++14",
    "CLANG_CXX_LIBRARY": "libc++"
  }
}
